{
  "name": "tree-sitter-hare",
  "version": "0.1.0",
  "description": "Hare grammar for tree-sitter",
  "main": "bindings/node",
  "types": "bindings/node",
  "scripts": {
    "generate": "tree-sitter generate",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "repository": {
    "type": "git",
    "url": "https://git.sr.ht/~omasanori/tree-sitter-hare"
  },
  "keywords": [
    "parser",
    "lexer",
    "hare"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "author": "Masanori Ogino",
  "license": "MIT",
  "homepage": "https://git.sr.ht/~omasanori/tree-sitter-hare",
  "devDependencies": {
    "tree-sitter-cli": "^0.22.6",
    "prebuildify": "^6.0.1"
  },
  "dependencies": {
    "node-gyp-build": "^4.8.1"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "tree-sitter": [
    {
      "scope": "source.hare",
      "injection-regex": "^(hare|ha)$"
    }
  ]
}
