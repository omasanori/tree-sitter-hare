{
  "name": "tree-sitter-hare",
  "version": "0.1.0",
  "description": "Hare grammar for tree-sitter",
  "main": "bindings/node",
  "types": "bindings/node",
  "scripts": {
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip",
    "build": "tree-sitter generate --no-bindings",
    "build-wasm": "tree-sitter build --wasm",
    "lint": "eslint grammar.js",
    "parse": "tree-sitter parse",
    "test": "tree-sitter test",
    "states": "tree-sitter generate --report-states-for-rule -"
  },
  "repository": {
    "type": "git",
    "url": "https://git.sr.ht/~omasanori/tree-sitter-hare"
  },
  "keywords": [
    "parser",
    "lexer",
    "hare"
  ],
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**"
  ],
  "author": "Masanori Ogino",
  "license": "MIT",
  "homepage": "https://git.sr.ht/~omasanori/tree-sitter-hare",
  "devDependencies": {
    "eslint": "^9.10.0",
    "eslint-config-google": "^0.14.0",
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.23.0"
  },
  "dependencies": {
    "node-gyp-build": "^4.8.2"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "tree-sitter": [
    {
      "scope": "source.hare",
      "injection-regex": "^(hare|ha)$"
    }
  ]
}
